{
 "compilerOptions": {
  "target": "es2018",
  "module": "commonjs",
  "lib": ["esnext"],
  "sourceMap": true,
  "outDir": "./dist",
  "esModuleInterop": true,
  "strict": true,
  "noImplicitAny": true,
  "moduleResolution": "node",
  "skipLibCheck": true,
  "resolveJsonModule": true
 },
 "include": ["./src/**/*"],
 "exclude": ["node_modules"]
}
